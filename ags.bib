@article{replicated_designs_sobol_seq,
    title = {Iterative construction of replicated designs based on Sobol' sequences},
    journal = {Comptes Rendus Mathematique},
    volume = {355},
    number = {1},
    pages = {10-14},
    year = {2017},
    issn = {1631-073X},
    doi = {https://doi.org/10.1016/j.crma.2016.11.013},
    url = {https://www.sciencedirect.com/science/article/pii/S1631073X16302576},
    author = {Laurent Gilquin and Lluís Antoni {Jiménez Rugama} and Élise Arnaud and Fred J. Hickernell and Hervé Monod and Clémentine Prieur},
    abstract = {In the perspective of estimating main effects of model inputs, two approaches are studied to iteratively construct replicated designs based on Sobol' sequences. Space-filling properties of the resulting designs are studied based on two criteria.
    Résumé
    Dans l'objectif d'estimer les effets principaux des paramètres d'un modèle, nous proposons d'étudier deux approches pour construire itérativement des plans répliqués à partir de séquences de Sobol'. Les propriétés de remplissement de l'espace des plans construits sont étudiées sur la base de deux critères.}
    }

